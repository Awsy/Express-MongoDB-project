    "restartable": "rs",
    "ignore": [".git", "node_modules/**/node_modules"],
    "verbose": false,
    "watch": ["./source", "./package.json", "./swagger"],
    "env": {
        "NODE_ENV": "development"
    },
    "exec": "node --experimental-modules ./source/index.mjs",
    "ext": "mjs, yaml, yml"
}